{
  "name": "@tls/ui-kit",
  "private": false,
  "license": "UNLICENSED",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "storybook-to-ghpages --existing-output-dir=storybook-static --ci",
    "deploy-storybook-manual": "npm run build-storybook && sed -i s=/assets/=/lina-essentials/assets/=g 'storybook-static/iframe.html' && npm run deploy-storybook",
    "commit": "git-cz"
  },
  "dependencies": {
    "jackspeak": "2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "7.3.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.3.2",
    "@storybook/manager-api": "^7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-vite": "^7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@storybook/theming": "^7.3.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "git-cz": "^4.9.0",
    "postcss": "^8.4.20",
    "storybook": "7.3.2",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1"
  }
}
